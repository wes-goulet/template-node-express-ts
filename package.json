{
  "name": "template-ts-node",
  "version": "0.0.1",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/node": "^12.0.10",
    "concurrently": "^4.1.1",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "typescript": "^3.5.2"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "start": "tsc && node dist/server.js",
    "watch-ts": "tsc -w",
    "compiled-debug": "nodemon --nolazy --inspect dist/server.js",
    "debug": "concurrently -k \"npm run watch-ts\" \"npm run compiled-debug\""
  }
}
