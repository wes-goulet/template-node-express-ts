{
  "name": "template-ts-node",
  "version": "1.0.0",
  "license": "MIT",
  "dependencies": {
    "express": "4.16.3",
    "express-graphql": "0.6.12",
    "graphql": "0.13.2"
  },
  "devDependencies": {
    "@types/express": "4.11.1",
    "@types/express-graphql": "0.6.1",
    "@types/graphql": "0.13.0",
    "@types/node": "9.6.6",
    "nodemon": "1.17.3",
    "prettier": "1.12.1",
    "ts-node": "6.0.0",
    "tslint": "5.9.1",
    "tslint-config-airbnb": "5.8.0",
    "tslint-config-prettier": "^1.12.0",
    "typescript": "2.8.3"
  },
  "scripts": {
    "build": "node_modules/.bin/tsc",
    "start": "node_modules/.bin/tsc && node dist/server.js",
    "debug": "node_modules/.bin/nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/server.ts"
  }
}
